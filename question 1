#ASSIGNMENT 5
# SOLUTION 1
marks=int(input("enter your marks please"))
if(marks<25):
    print("F grade")
if(marks>25 and marks<45):
    print("E grade")
if(marks>45 and marks<50):
    print("D grade")
if(marks>50 and marks<60):
    print("C grade")
if(marks>60 and marks<80):
    print("B grade")
if(marks>80):
    print("A grade")
